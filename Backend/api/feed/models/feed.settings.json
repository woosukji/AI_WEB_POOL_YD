{
  "kind": "collectionType",
  "collectionName": "feeds",
  "info": {
    "name": "Feed"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "author": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "title": {
      "type": "string"
    },
    "content": {
      "type": "richtext"
    },
    "commentCount": {
      "type": "integer"
    },
    "likesCount": {
      "type": "integer"
    },
    "image": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    }
  }
}
